{
  "name": "@anthropic/ash-server",
  "version": "1.0.0",
  "description": "ASH Protocol Server SDK - Context Management and Request Verification",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic/ash-core": "file:../ash-core"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "express": "^4.18.2",
    "fastify": "^4.24.0",
    "tsup": "^8.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "express": "^4.18.0",
    "fastify": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "express": { "optional": true },
    "fastify": { "optional": true }
  },
  "keywords": [
    "ash",
    "middleware",
    "express",
    "fastify",
    "security"
  ],
  "license": "Proprietary",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3meam/ash.git",
    "directory": "packages/ash-server"
  }
}
