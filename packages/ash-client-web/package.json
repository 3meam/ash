{
  "name": "@anthropic/ash-client-web",
  "version": "1.0.0",
  "description": "ASH Protocol Client SDK - Browser and Node.js Client",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "browser": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic/ash-core": "file:../ash-core"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "ash",
    "client",
    "browser",
    "integrity",
    "security"
  ],
  "license": "Proprietary",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3meam/ash.git",
    "directory": "packages/ash-client-web"
  },
  "sideEffects": false
}
