{
  "description": "JSON Canonicalization Test Vectors for ASH",
  "version": "1.0.0",
  "vectors": [
    {
      "name": "simple_object_key_ordering",
      "description": "Object keys must be sorted lexicographically",
      "input": "{\"z\":1,\"a\":2}",
      "expected": "{\"a\":2,\"z\":1}"
    },
    {
      "name": "nested_object_key_ordering",
      "description": "Nested object keys must also be sorted",
      "input": "{\"b\":{\"d\":4,\"c\":3},\"a\":1}",
      "expected": "{\"a\":1,\"b\":{\"c\":3,\"d\":4}}"
    },
    {
      "name": "deeply_nested_object",
      "description": "Deep nesting with key ordering",
      "input": "{\"z\":{\"y\":{\"x\":1,\"w\":2}}}",
      "expected": "{\"z\":{\"y\":{\"w\":2,\"x\":1}}}"
    },
    {
      "name": "array_order_preserved",
      "description": "Array element order must be preserved",
      "input": "{\"arr\":[3,1,2]}",
      "expected": "{\"arr\":[3,1,2]}"
    },
    {
      "name": "array_of_objects",
      "description": "Array of objects with key ordering",
      "input": "{\"items\":[{\"z\":1,\"a\":2},{\"y\":3,\"b\":4}]}",
      "expected": "{\"items\":[{\"a\":2,\"z\":1},{\"b\":4,\"y\":3}]}"
    },
    {
      "name": "null_value",
      "description": "Null values preserved",
      "input": "{\"b\":null,\"a\":1}",
      "expected": "{\"a\":1,\"b\":null}"
    },
    {
      "name": "boolean_values",
      "description": "Boolean values preserved",
      "input": "{\"b\":true,\"a\":false}",
      "expected": "{\"a\":false,\"b\":true}"
    },
    {
      "name": "empty_object",
      "description": "Empty object",
      "input": "{}",
      "expected": "{}"
    },
    {
      "name": "empty_array",
      "description": "Empty array",
      "input": "[]",
      "expected": "[]"
    },
    {
      "name": "whitespace_removed",
      "description": "All whitespace must be removed",
      "input": "{ \"z\" : 1 , \"a\" : 2 }",
      "expected": "{\"a\":2,\"z\":1}"
    },
    {
      "name": "newlines_removed",
      "description": "Newlines and formatting removed",
      "input": "{\n  \"z\": 1,\n  \"a\": 2\n}",
      "expected": "{\"a\":2,\"z\":1}"
    },
    {
      "name": "integer_zero",
      "description": "Integer zero",
      "input": "{\"value\":0}",
      "expected": "{\"value\":0}"
    },
    {
      "name": "negative_integer",
      "description": "Negative integer",
      "input": "{\"value\":-42}",
      "expected": "{\"value\":-42}"
    },
    {
      "name": "decimal_number",
      "description": "Decimal number",
      "input": "{\"value\":3.14}",
      "expected": "{\"value\":3.14}"
    },
    {
      "name": "string_with_unicode",
      "description": "Unicode strings with NFC normalization",
      "input": "{\"name\":\"cafÃ©\"}",
      "expected": "{\"name\":\"cafÃ©\"}"
    },
    {
      "name": "string_with_emoji",
      "description": "String containing emoji",
      "input": "{\"emoji\":\"Hello ðŸ‘‹\"}",
      "expected": "{\"emoji\":\"Hello ðŸ‘‹\"}"
    },
    {
      "name": "string_with_escape",
      "description": "String with escape sequences",
      "input": "{\"text\":\"line1\\nline2\"}",
      "expected": "{\"text\":\"line1\\nline2\"}"
    },
    {
      "name": "empty_string",
      "description": "Empty string value",
      "input": "{\"empty\":\"\"}",
      "expected": "{\"empty\":\"\"}"
    },
    {
      "name": "mixed_types",
      "description": "Object with mixed value types",
      "input": "{\"str\":\"hello\",\"num\":42,\"bool\":true,\"null\":null,\"arr\":[1,2]}",
      "expected": "{\"arr\":[1,2],\"bool\":true,\"null\":null,\"num\":42,\"str\":\"hello\"}"
    },
    {
      "name": "numeric_string_keys",
      "description": "String keys that look like numbers",
      "input": "{\"10\":\"ten\",\"2\":\"two\",\"1\":\"one\"}",
      "expected": "{\"1\":\"one\",\"10\":\"ten\",\"2\":\"two\"}"
    },
    {
      "name": "special_characters_in_keys",
      "description": "Keys with special characters",
      "input": "{\"a-b\":1,\"a_c\":2,\"a.d\":3}",
      "expected": "{\"a-b\":1,\"a.d\":3,\"a_c\":2}"
    },
    {
      "name": "large_integer",
      "description": "Large integer value",
      "input": "{\"big\":9007199254740991}",
      "expected": "{\"big\":9007199254740991}"
    },
    {
      "name": "scientific_notation_avoided",
      "description": "Numbers should not use scientific notation",
      "input": "{\"value\":0.0001}",
      "expected": "{\"value\":0.0001}"
    }
  ]
}
